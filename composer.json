{
  "name": "dsandrade/laravel-ifx",
  "description": "Informix DB driver for Laravel 6",
  "keywords": ["informix", "laravel", "laravel 6", "ifx" ],
  "license": "MIT",
  "version": "dev-master",
  "authors": [
    {
      "name": "dsandrade",
      "email": "dsandrade777@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "~5.0|~6.0",
    "illuminate/database": "~5.0|~6.0",
    "illuminate/pagination": "~5.0|~6.0",
    "illuminate/encryption": "~5.0|~6.0",
    "guzzlehttp/guzzle": "6.0.*|6.1.*|6.2.*|6.3.*|6.4.*|6.5.*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpspec/phpspec": "~2.1",
    "phpunit/phpunit": "~4.0|^6.5|^7.0|^8.0",
    "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*|4.*",
    "mockery/mockery": "~0.9.4|^1.0|^1.1"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Poyii\\Informix\\InformixDBServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Poyii\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
